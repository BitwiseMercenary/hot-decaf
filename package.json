{
  "name": "hot-coffee-loader",
  "version": "1.0.0",
  "description": "Hot load coffee modules as JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-fix": "standardx --fix"
  },
  "author": "Robert Mercado",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:DarkseidOmega/hot-coffee-loader.git"
  },
  "dependencies": {
    "coffeescript": "^2.5.1",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "create-react-class": "^15.7.0",
    "react-dom-factories": "^1.0.2",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "standardx": "^7.0.0"
  },
  "eslintConfig": {
    "rules": {
      "camelcase": 0,
      "semi": [
        2,
        "always"
      ],
      "quotes": [
        2,
        "double",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-runtime"
    ]
  }
}
